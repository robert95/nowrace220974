{% extends 'base.html.twig' %}

{% block body %}
    <h1>Trackpoint</h1>

    <table>
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ trackPoint.id }}</td>
            </tr>
            <tr>
                <th>Ordernumber</th>
                <td>{{ trackPoint.orderNumber }}</td>
            </tr>
            <tr>
                <th>Lat</th>
                <td>{{ trackPoint.lat }}</td>
            </tr>
            <tr>
                <th>Lng</th>
                <td>{{ trackPoint.lng }}</td>
            </tr>
            <tr>
                <th>Type</th>
                <td>{{ trackPoint.type }}</td>
            </tr>
        </tbody>
    </table>

    <ul>
        <li>
            <a href="{{ path('trackpoint_index') }}">Back to the list</a>
        </li>
        <li>
            <a href="{{ path('trackpoint_edit', { 'id': trackPoint.id }) }}">Edit</a>
        </li>
        <li>
            {{ form_start(delete_form) }}
                <input type="submit" value="Delete">
            {{ form_end(delete_form) }}
        </li>
    </ul>
{% endblock %}
